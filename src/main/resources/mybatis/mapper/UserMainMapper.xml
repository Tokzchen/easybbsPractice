<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.easybbsweb.mapper.UserMainMapper">

        <insert id="insertUserMain">
            INSERT INTO user_main
            <trim prefix="(" suffix=")" suffixOverrides=",">
                <if test="userId != null and userId != ''">user_id,</if>
                <if test="aidId != null">aid_id,</if>
                <if test="lawyerId != null">lawyer_id,</if>
                <if test="createTime != null">create_time,</if>
                <if test="emailCode != null and emailCode != ''">email_code,</if>
                <if test="checkCode != null and checkCode != ''">check_code,</if>
                <if test="lastUpdateTime!=null ">last_update_time</if >
            </trim>

            VALUES
            <trim prefix ="(" suffix =")" suffixOverrides=",">
                <if test ="userId!=null ">#{userId},</if>
                <if test ="aidId!=null ">#{aidId},</if>
                <if test ="lawyerId!=null ">#{lawyerId},</if>
                <if test ="createTime!=null ">#{createTime},</if>
                <if test ="emailCode!=null ">#{emailCode},</if>
                <if test ="checkCode!=null ">#{checkCode},</if>
                <if test = "lastUpdateTime! = null" > #{lastUpdateTime} </if>
            </trim>
        </insert>

</mapper>